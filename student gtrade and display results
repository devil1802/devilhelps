#include<stdio.h>
int main( 
{
    int subject1, subject2, subject3, subject4, subject5;
    int total;
    float average;
    char grade; 
    printf("Enter the marks for 5 subjects: \n");
    printf("Subject 1: ");
    scanf("%d", &subject1);
    printf("Subject 2: ");
    scanf("%d", &subject2);
    printf("Subject 3: ");
    scanf("%d", &subject3);
    printf("Subject 4: ");
    scanf("%d", &subject4);
    printf("Subject 5: ");
    scanf("%d", &subject5);
    total = subject1 + subject2 + subject3 + subject4 + subject5;
    average = total / 5.0;
    if(average >= 90)
	{
        grade = 'A';
    } 
	else if(average >= 80)
    {
        grade = 'B';
    }
	 else if(average >= 70)
	{
        grade = 'C';
    } 
	else if(average >= 60) 
	{
        grade = 'D';
    }
	 else 
    {
        grade ='F';
    }
    printf("\nTotal Marks: %d\n", total);
    printf("Average Marks: %.2f\n", average);
    printf("Grade: %c\n", grade);
    return 0;
}
